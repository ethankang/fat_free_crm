- @global_lists = List.where(:user_id => nil)
- @personal_lists = List.where(:user_id => current_user.id)
- user_relation_tables = @current_tab
- tab_select = ["tasks", "accounts", "leads", "contacts", "opportunities"]
- @user_lists = User.all
#lists
  = render "lists/sidebar", lists: @global_lists
  - if current_user.admin? && tab_select.include?(user_relation_tables)
    = render 'lists/sidebar_users', lists: @user_lists,user_relation_tables: user_relation_tables
  = render "lists/sidebar", lists: @personal_lists, personal: true
